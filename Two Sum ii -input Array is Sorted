class Solution(object):
    def twoSum(self, numbers, target):
        i = 0
        j = len(numbers)-1
        while(i<j):
            sum = numbers[i] + numbers[j]
            if sum == target:
                a = []
                a.append(i+1)
                a.append(j+1)
                return a
            elif sum < target:
                i+=1
            else:
                j-=1
